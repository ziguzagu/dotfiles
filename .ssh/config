Host *
  ForwardAgent yes
  ServerAliveInterval 60
  AddKeysToAgent yes
  UseKeychain yes

Host github.com
  Compression yes

Match host 127.0.0.1
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentitiesOnly yes
  LogLevel FATAL
  ForwardAgent yes
  Compression no

Include conf.d/hosts/*
